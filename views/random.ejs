<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>APP15-1</title>
    <script src="/axios/axios.min.js"></script>
  </head>
  <body>
    <button type="button" onclick="ajaxSubmit()">Random</button>
    <script>
      function ajaxSubmit() {
        axios({
          url: "/random",
          method: "get",
          params: { min: 1, max: 10 },
          timeout: 3000,
        })
          .then((res) => {
            alert("Random Number : " + res.data.result);
          })
          .catch((error) => alert(error));
      }
    </script>
  </body>
</html>
